<UserControl x:Class="DataWranglerInterface.ShotRecording.CameraInfoControl"
			 x:Name="CameraInfoControlInstance"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataWranglerInterface.ShotRecording"
             xmlns:dataWranglerInterface="clr-namespace:DataWranglerInterface"
             xmlns:dataWranglerCommonWpf="clr-namespace:DataWranglerCommonWPF;assembly=DataWranglerCommonWPF"
             mc:Ignorable="d" 
             d:DesignHeight="256" d:DesignWidth="256">
	<StackPanel>
		<ItemsControl ItemsSource="{Binding ActiveCameras, ElementName=CameraInfoControlInstance}">
		</ItemsControl>
	</StackPanel>
</UserControl>
