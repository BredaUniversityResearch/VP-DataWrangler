             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataWranglerInterface.ShotRecording"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<StackPanel>
        <DockPanel>
			<Label>File Sources</Label>
			<Button x:Name="AddFileSourceButton" HorizontalAlignment="Right" VerticalAlignment="Top" Height="21" Width="40" Click="AddFileSourceButton_Click">Add</Button>
        </DockPanel>
        <StackPanel x:Name="FileSourceControl">

        </StackPanel>
    </StackPanel>
</UserControl>
