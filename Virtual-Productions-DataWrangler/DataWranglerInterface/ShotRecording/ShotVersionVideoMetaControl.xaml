<UserControl x:Class="DataWranglerInterface.ShotRecording.ShotVersionVideoMetaControl"
             x:Name="VideoMetaUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataWranglerInterface.ShotRecording"
             xmlns:xamlUtility="clr-namespace:DataWranglerInterface.XamlUtility"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<StackPanel xamlUtility:Spacing.Vertical="10">
		<ComboBox x:Name="VideoSource" SelectedItem="{Binding ElementName=VideoMetaUserControl, Path=CurrentMeta.Source}"/>
    </StackPanel>
</UserControl>
