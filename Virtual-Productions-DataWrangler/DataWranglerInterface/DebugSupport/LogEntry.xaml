<UserControl x:Class="DataWranglerInterface.DebugSupport.LogEntry"
             x:Name="LogEntryInstance"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataWranglerInterface.DebugSupport"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
		<TextBlock Text="{Binding ElementName=LogEntryInstance, Path=Source}"></TextBlock>
		<TextBlock Text="{Binding ElementName=LogEntryInstance, Path=Severity}"></TextBlock>
		<TextBlock Text="{Binding ElementName=LogEntryInstance, Path=Message}"></TextBlock>
    </Grid>
</UserControl>
